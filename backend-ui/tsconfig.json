{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ],
      "@utils/*": [
        "src/app/utils/*"
      ],
      "@ui/*": [
        "src/app/ui/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@data_types/*": [
        "src/app/types/*"
      ],
      "@store/*": [
        "src/app/store/*"
      ],
      "@store_reducers/*": [
        "src/app/store/reducers/*"
      ],
      "@store_actions/*": [
        "src/app/store/actions/*"
      ],
      "@store_effects/*": [
        "src/app/store/effects/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "lib": [
      "es2019",
      "dom"
    ],
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
